#include <stdio.h>

void printPascalTriangle(int rows) {
    int i, j, num = 1;


    for (i = 0; i < rows; i++) {

        for (j = 0; j < rows - i - 1; j++) {
            printf(" ");
        }

        for (j = 0; j <= i; j++) {
            if (j == 0 || i == 0) {
                num = 1;
            } else {
                num = num * (i - j + 1) / j;
            }
            printf("%d ", num);
        }

        printf("\n");
    }
}

int main() {
	printf("G.Ajay kumar-192210590\n");
    int rows;

    printf("Enter the number of rows: ");
    scanf("%d", &rows);

    printPascalTriangle(rows);

    return 0;
}
