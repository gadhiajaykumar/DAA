#include <stdio.h>

int main() {
	printf("G.Ajay kumar-192210590\n");
    int m, n;
    printf("Enter the number of rows for the first matrix (m): ");
    scanf("%d", &m);
    printf("Enter the number of columns for the first matrix (n): ");
    scanf("%d", &n);

    int p, q;
    printf("Enter the number of rows for the second matrix (p): ");
    scanf("%d", &p);
    printf("Enter the number of columns for the second matrix (q): ");
    scanf("%d", &q);

    if (n != p) {
        printf("Matrices cannot be multiplied.\n");
        return 1;
    }

    int matrix1[m][n];
    int matrix2[p][q];
    int result[m][q];

    printf("Enter elements of the first matrix:\n");
    for (int i = 0; i < m; i++) {
        for (int j = 0; j < n; j++) {
            scanf("%d", &matrix1[i][j]);
        }
    }

    printf("Enter elements of the second matrix:\n");
    for (int i = 0; i < p; i++) {
        for (int j = 0; j < q; j++) {
            scanf("%d", &matrix2[i][j]);
        }
    }

    for (int i = 0; i < m; i++) {
        for (int j = 0; j < q; j++) {
            result[i][j] = 0;
        }
    }

    for (int i = 0; i < m; i++) {
        for (int j = 0; j < q; j++) {
            for (int k = 0; k < n; k++) {
                result[i][j] += matrix1[i][k] * matrix2[k][j];
            }
        }
    }
    printf("Resultant matrix:\n");
    for (int i = 0; i < m; i++) 
        for (int j = 0; j < q; j++) {
            printf("%d ", result[i][j]);
        }
        printf("\n");
}
 
